{
    "cmake.buildDirectory": "${workspaceFolder}/build",
    "cmake.configureOnOpen": true,
    "cmake.buildTask": true,
    "cmake.defaultVariants": {
        "buildType": {
            "default": "debug",
            "choices": {
                "debug": {
                    "short": "Debug",
                    "long": "Debug build with debug symbols",
                    "buildType": "Debug"
                },
                "release": {
                    "short": "Release",
                    "long": "Release build with optimizations",
                    "buildType": "Release"
                },
                "relwithdebinfo": {
                    "short": "RelWithDebInfo",
                    "long": "Release with debug info",
                    "buildType": "RelWithDebInfo"
                }
            }
        }
    },
    "files.associations": {
        "*.cmake": "cmake",
        "CMakeLists.txt": "cmake",
        "cstdint": "cpp",
        "numbers": "cpp",
        "*.inc": "cpp",
        "cmath": "cpp",
        "complex": "cpp",
        "compare": "cpp",
        "__locale": "cpp",
        "__nullptr": "cpp",
        "initializer_list": "cpp",
        "new": "cpp",
        "optional": "cpp",
        "stdexcept": "cpp",
        "typeinfo": "cpp",
        "__bit_reference": "cpp",
        "__hash_table": "cpp",
        "__node_handle": "cpp",
        "__split_buffer": "cpp",
        "__threading_support": "cpp",
        "__verbose_abort": "cpp",
        "array": "cpp",
        "bitset": "cpp",
        "cctype": "cpp",
        "charconv": "cpp",
        "clocale": "cpp",
        "condition_variable": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "memory": "cpp",
        "forward_list": "cpp",
        "ios": "cpp",
        "iosfwd": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "locale": "cpp",
        "mutex": "cpp",
        "ostream": "cpp",
        "print": "cpp",
        "queue": "cpp",
        "ratio": "cpp",
        "semaphore": "cpp",
        "streambuf": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "tuple": "cpp",
        "unordered_map": "cpp",
        "variant": "cpp",
        "vector": "cpp",
        "algorithm": "cpp",
        "cuda_std_post": "cpp",
        "__string": "cpp",
        "regex": "cpp"
    },
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "editor.inlineSuggest.enabled": false
}
